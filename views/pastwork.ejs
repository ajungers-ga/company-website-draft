<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Past Work</title>
    <link rel="stylesheet" href="/css/app.css">
</head>
<body>

    <%- include('partials/nav') %>


    <header>
        <h1>Past Work</h1>
    </header>

    <main>
        <ul>
            <% projects.forEach(function(project) { %>
                <li>
                    <a href="/pastwork/<%= project.id %>">
                        <%= project.title %>
                    </a>
                </li>
            <% }); %>
        </ul>
    </main>

</body>
</html>
